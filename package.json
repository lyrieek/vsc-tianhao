{
	"name": "vsc-tianhao",
	"displayName": "Tianhao",
	"description": "tianhao vscode extension",
	"version": "0.0.2",
	"homepage": "https://github.com/lyrieek/vsc-tianhao/blob/master/README.md",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"engines": {
		"node": ">=0.10.0",
		"vscode": "^1.30.0"
	},
	"main": "./out/extension",
	"remoteConfigPath": "./remote.json",
	"contributes": {
		"commands": [{
				"title": "Toggle Case",
				"command": "tianhao.toggleCase"
			},
			{
				"title": "Test",
				"command": "tianhao.test"
			},
			{
				"title": "Remote",
				"command": "tianhao.remote"
			},
			{
				"title": "Alert Timestamp",
				"command": "tianhao.time"
			}
		],
		"keybindings": [{
			"command": "tianhao.toggleCase",
			"key": "ctrl+shift+u",
			"when": "editorTextFocus"
		}]
	},
	"scripts": {
		"watch": "tsc -watch -p ./",
		"build": "vsce package",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.21",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"ssh2": "^0.6.1"
	}
}
