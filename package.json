{
	"name": "vsc-tianhao",
	"displayName": "Tianhao",
	"description": "tianhao vscode extension",
	"version": "0.0.1",
	"publisher": "Tianhao",
	"engines": {
		"vscode": "^1.28.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tianhao.toggleCase",
		"onCommand:tianhao.test"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [{
			"title": "Toggle Case",
			"command": "tianhao.toggleCase"
		},{
			"title": "Test",
			"command": "tianhao.test"
		}],
		"keybindings": [{
			"command": "tianhao.toggleCase",
			"key": "ctrl+shift+u",
			"when": "editorTextFocus"
		}]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.21",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
